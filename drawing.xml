<config>
  <node name="draw_window">
    <node name="draw_button">
      <font>Microsoft MHei, 72px</font>
      <bounds>500, 930, 800, 80</bounds>
      <start_title>Go ! ! !</start_title>
      <stop_title>Stop Here!</stop_title>
    </node>

    <node name="draw_view">
      <font>Microsoft MHei, 80px</font>
      <bounds>500, 100, 800, 800</bounds>
      <text_color>255, 1, 1, 0</text_color>
      <background_color>255, 255, 255, 255</background_color>
    </node>
    <background>255, 255, 255, 255</background>
  </node>

  <node name="rounds">
    <node name="round0">
      <node name="animations" frame_count="2">
        <node name="animation" key="" frame="0">
          <path>d:\data\camera.png</path>
          <init_bounds>100, 100, 300, 300</init_bounds>
          <target_bounds>900, 100, 600, 300</target_bounds>
          <duration>1000</duration>
        </node>

        <node name="animation" key="" frame="0">
          <path>d:\data\iphone6.png</path>
          <init_bounds>100, 300, 300, 300</init_bounds>
          <target_bounds>900, 300, 300, 300</target_bounds>
          <duration>1000</duration>
        </node>

        <node name="animation" key="" frame="1">
          <path>d:\data\iphone6.png</path>
          <init_bounds>100, 300, 300, 300</init_bounds>
          <target_bounds>900, 300, 300, 300</target_bounds>
          <duration>1000</duration>
        </node>
      </node>
    </node>

    <node name="round1">
      <node name="animations" frame_count="1">
        <node name="animation" key="" frame="0">
          <path>camera.png</path>
          <init_bounds>100, 100, 300, 300</init_bounds>
          <target_bounds>800, 800, 300, 300</target_bounds>
          <duration>1000</duration>
        </node>
      </node>
    </node>

    <node name="round2">
      <node name="animations" frame_count="1">
        <node name="animation" key="" frame="0">
          <path>camera.png</path>
          <init_bounds>100, 100, 300, 300</init_bounds>
          <target_bounds>800, 800, 300, 300</target_bounds>
          <duration>1000</duration>
        </node>
      </node>
    </node>

    <node name="round3">
      <node name="animations" frame_count="1">
        <node name="animation" key="" frame="0">
          <path>camera.png</path>
          <init_bounds>100, 100, 300, 300</init_bounds>
          <target_bounds>800, 800, 300, 300</target_bounds>
          <duration>1000</duration>
        </node>
      </node>
    </node>
  </node>
</config>
